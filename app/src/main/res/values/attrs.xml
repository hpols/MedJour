<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SoundPreference">
        <attr name="ringtoneType">
            <!-- Ringtones. -->
            <flag name="ringtone" value="1" />
            <!-- Notification sounds. -->
            <flag name="notification" value="2" />
            <!-- Alarm sounds. -->
            <flag name="alarm" value="4" />
            <!-- All available ringtone sounds. -->
            <flag name="all" value="7" />
        </attr>
        <attr name="appOwnSounds" format="reference" />
        <attr name="appOwnSoundsTitles" format="reference" />
    </declare-styleable>
</resources>